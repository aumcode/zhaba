#<laconf>
    compiler
    {
    using { ns="System.Linq" }
    using { ns="System.Collections.Generic" }
    using { ns="NFX" }
    using { ns="NFX.DataAccess.CRUD" }
    using { ns="NFX.Wave" }
    using { ns="NFX.Wave.Templatization" }
    using { ns="Zhaba" }
    using { ns="Zhaba.Data.Forms" }
    base-class-name="Zhaba.Web.Pages.SiteFormMaster<ComponentForm>"
    namespace="Zhaba.Web.Pages"
    }
#</laconf>

#[class]
  public ComponentPage(ComponentForm form, Exception error)
    : base(form, error)
 {
    Title = "{0} Component".Args((form.FormMode == FormMode.Edit) ? "Edit" : "Create");
  }


#[override renderControls()]
  <div>
    <div class="form-group">
      <div class="data-wv" data-wv-fname="Name"></div>
    </div>
    <div class="form-group">
      <div class="data-wv" data-wv-fname="Description"></div>
    </div>

  	<div class="ln_solid"></div>
    <div class="form-group cntr form-btns-wrapper">
      <input class="btn btn-primary btn-std" type="submit" value="Save" />
      <a class="btn btn-primary btn-std" href="?[:URIS.ForPROJECT_ISSUES(Form.ProjectID)]">Cancel</a>
    </div>
  </div>
